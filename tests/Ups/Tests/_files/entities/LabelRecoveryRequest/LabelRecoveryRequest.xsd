<xsd:schema elementFormDefault="unqualified" xmlns:ups="http://www.ups.com/XMLSchema"            xmlns:xsd="http://www.w3.org/2001/XMLSchema">    <xsd:element name="LabelRecoveryRequest">        <xsd:complexType>            <xsd:sequence>                <xsd:element ref="Request"/>                <xsd:element minOccurs="0" name="LabelSpecification" type="LabelSpecificationType"/>                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Translate" type="TranslateType"/>                <xsd:element minOccurs="0" name="LabelDelivery" type="LabelDeliveryType"/>                <xsd:choice>                    <xsd:element name="TrackingNumber" type="xsd:string"/>                    <xsd:sequence>                        <xsd:element name="ReferenceNumber" type="ReferenceNumberType"/>                        <xsd:element name="ShipperNumber" type="xsd:string"/>                    </xsd:sequence>                </xsd:choice>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:complexType name="LabelSpecificationType">        <xsd:sequence>            <xsd:element minOccurs="0" name="HTTPUserAgent" type="xsd:string"/>            <xsd:element name="LabelImageFormat" type="CodeType"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="CodeType">        <xsd:sequence>            <xsd:element name="Code" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="TranslateType">        <xsd:sequence>            <xsd:element name="LanguageCode" type="xsd:string"/>            <xsd:element minOccurs="0" name="DialectCode" type="xsd:string"/>            <xsd:element minOccurs="0" name="Code" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="LabelDeliveryType">        <xsd:sequence>            <xsd:element minOccurs="0" ref="LabelLinkIndicator"/>            <xsd:element minOccurs="0" ref="ResendEMailIndicator"/>            <xsd:element minOccurs="0" name="EMailMessage" type="EMailMessageType"/>        </xsd:sequence>    </xsd:complexType>    <xsd:element name="LabelLinkIndicator">        <xsd:complexType/>    </xsd:element>    <xsd:element name="ResendEMailIndicator">        <xsd:complexType/>    </xsd:element>    <xsd:complexType name="ReferenceNumberType">        <xsd:sequence>            <xsd:element name="Value" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="EMailMessageType">        <xsd:sequence>            <xsd:element name="EMailAddress" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:element name="Request">        <xsd:complexType>            <xsd:sequence>                <xsd:element minOccurs="0" ref="TransactionReference"/>                <xsd:element name="RequestAction" type="xsd:string"/>                <xsd:element maxOccurs="unbounded" minOccurs="0" name="RequestOption" type="xsd:string"                             ups:usage="notused"/>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:element name="TransactionReference">        <xsd:complexType>            <xsd:sequence/>        </xsd:complexType>    </xsd:element></xsd:schema>