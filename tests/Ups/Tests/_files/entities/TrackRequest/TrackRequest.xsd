<xsd:schema xmlns:ups="http://www.ups.com/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema"            elementFormDefault="unqualified">    <xsd:element name="TrackRequest">        <xsd:complexType>            <xsd:sequence>                <xsd:element name="Request" type="RequestTYpe"/>                <xsd:choice>                    <xsd:element name="TrackingNumber" type="xsd:string"/>                    <xsd:element name="ShipmentIdentificationNumber" type="xsd:string"/>                    <xsd:element name="CandidateBookmark" type="xsd:string"/>                    <xsd:sequence>                        <xsd:element name="ReferenceNumber" type="ReferenceNumberType"/>                        <xsd:element name="PickupDateRange" type="PickupDateRangeType" minOccurs="0"/>                        <xsd:element name="ShipperNumber" type="xsd:string" minOccurs="0"/>                        <xsd:element name="DestinationPostalCode" type="xsd:string" minOccurs="0"/>                        <xsd:element name="DestinationCountryCode" type="xsd:string" minOccurs="0"/>                        <xsd:element name="OriginPostalCode" type="xsd:string" minOccurs="0"/>                        <xsd:element name="OriginCountryCode" type="xsd:string" minOccurs="0"/>                        <xsd:element name="ShipmentType" type="CodeType" minOccurs="0"/>                    </xsd:sequence>                </xsd:choice>                <xsd:element name="TrackingOption" type="xsd:string" minOccurs="0"/>                <xsd:element name="UPSWorldWideExpressFreightShipment" type="xsd:string" minOccurs="0"/>                <xsd:element name="IncludeFreight" type="xsd:string" minOccurs="0"/>                <xsd:element name="ShipperAccountInfo" type="ShipperAccountInfoType" minOccurs="0"/>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:complexType name="CodeType">        <xsd:sequence>            <xsd:element name="Code" type="xsd:string"/>            <xsd:element name="Description" type="xsd:string" minOccurs="0"/>        </xsd:sequence>    </xsd:complexType>    <xsd:element name="EventFilter" type="xsd:string"/>    <xsd:complexType name="PickupDateRangeType">        <xsd:sequence>            <xsd:element name="BeginDate" type="xsd:string"/>            <xsd:element name="EndDate" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="ReferenceNumberType">        <xsd:sequence>            <xsd:element name="Value" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="RequestTYpe">        <xsd:sequence>            <xsd:element name="TransactionReference" type="TransactionReferenceType"/>            <xsd:element name="RequestAction" type="xsd:string"/>            <xsd:element name="RequestOption" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="ShipperAccountInfoType">        <xsd:sequence>            <xsd:element name="PostalCode" type="xsd:string" minOccurs="0"/>            <xsd:element name="CountryCode" type="xsd:string"/>        </xsd:sequence>    </xsd:complexType>    <xsd:complexType name="TransactionReferenceType">        <xsd:sequence>            <xsd:element name="CustomerContext" type="xsd:string" minOccurs="0"/>            <xsd:element name="TransactionIdentifier" type="xsd:string" minOccurs="0"/>            <xsd:element name="ToolVersion" type="xsd:string" minOccurs="0"/>        </xsd:sequence>    </xsd:complexType></xsd:schema>