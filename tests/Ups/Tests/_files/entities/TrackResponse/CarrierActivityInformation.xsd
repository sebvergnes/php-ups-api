<xsd:schema xmlns:ups="http://www.ups.com/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema"            elementFormDefault="unqualified">    <xsd:element name="CarrierActivityInformation">        <xsd:complexType>            <xsd:sequence>                <xsd:element name="CarrierId" type="xsd:string" minOccurs="0"/>                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>                <xsd:element name="Status" type="xsd:string" minOccurs="0"/>                <xsd:element name="Arrival" type="DateTime" minOccurs="0"/>                <xsd:element name="Departure" type="DateTime" minOccurs="0"/>                <xsd:element name="OriginPort" type="xsd:string" minOccurs="0"/>                <xsd:element name="DestinationPort" type="xsd:string" minOccurs="0"/>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:complexType name="DateTime">        <xsd:sequence>            <xsd:element name="Date" type="xsd:string"/>            <xsd:element name="Time" type="xsd:string" minOccurs="0"/>        </xsd:sequence>    </xsd:complexType></xsd:schema>